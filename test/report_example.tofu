main {}class Student {  var iq : Integer;  def init() : Unit = { iq = 0; }  def learn(x : Integer) = { iq = iq + x; }}class Mathematician extends Student {  var number : Integer;  def setNumber(x : Integer) { number = x; }  def init() : Unit = { iq = 130; number = 0; }}class ComputerScientist extends Mathematician {  var coding : Boolean;  def coding() : Boolean = { return coding; }  def init() : Unit = { iq = 256; number = 1024; coding = false; }}